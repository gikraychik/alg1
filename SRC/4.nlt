# переход в standby режим

INCLUDE(regs.inc)

# реле
IMOVE(0,S2)
IMOVE(0,S3)
IMOVE(0,S4)
	
IMOVE(0,IND1)						# индикаторы
IMOVE(0,IND5)
IMOVE(0,IND4)

IMOVE(0,LED0)						# сегментный индикатор
IMOVE(0,LED1)
IMOVE(0,LED2)
IMOVE(0,LED3)

IMOVE(0,VIDEO_MODE)					# сброс экрана

MOVE(BTN_UP,TMP0)
MOVE(BTN_DOWN,TMP0)
MOVE(BTN_ENTER,TMP0)
MOVE(LONG_BTN_ENTER,TMP0)

AND(LONG_BTN_UP, LONG_BTN_DOWN, TMP1)
IMOVE(0, CTRL_STATE)
IMOVE(5, TMP0)
IMUX2(TMP1, NETLIST_SELECT, 4, 3)